# 환경 변수 설정 (.env.example 복사해서 .env로 사용)
# Docker Compose에서 사용할 환경 변수

# Gemini AI API 키 (필수)
GEMINI_API_KEY=your_gemini_api_key_here

# 서버 설정
NODE_ENV=development
BACKEND_PORT=3001
FRONTEND_PORT=5173

# Redis 설정
REDIS_PORT=6379

# Chrome Extension ID (빌드 후 설정)
CHROME_EXTENSION_ID=your-extension-id-here
